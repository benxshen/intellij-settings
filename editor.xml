<application>
  <component name="CodeInsightSettings">
    <option name="ADD_UNAMBIGIOUS_IMPORTS_ON_THE_FLY" value="true" />
    <EXCLUDED_PACKAGE NAME="org.junit.Assert.assertThat" />
  </component>
  <component name="DefaultFont">
    <option name="FONT_SIZE" value="12" />
    <option name="FONT_FAMILY" value="Input Mono" />
    <option name="FONT_REGULAR_SUB_FAMILY" value="Medium" />
    <option name="FONT_BOLD_SUB_FAMILY" value="Bold" />
    <option name="LINE_SPACING" value="1.1" />
    <option name="SECONDARY_FONT_FAMILY" value="WenQuanYi Micro Hei Mono" />
  </component>
  <component name="EditorSettings">
    <option name="languageBreadcrumbsMap">
      <map>
        <entry key="Groovy" value="true" />
        <entry key="HTML" value="true" />
        <entry key="JAVA" value="false" />
        <entry key="JSON" value="true" />
        <entry key="XHTML" value="true" />
        <entry key="XML" value="true" />
        <entry key="kotlin" value="false" />
        <entry key="yaml" value="true" />
      </map>
    </option>
    <option name="USE_SOFT_WRAPS" value="MAIN_EDITOR" />
    <option name="SOFT_WRAP_FILE_MASKS" value="*.md; *.txt; *.rst; *.adoc" />
    <option name="IS_ENSURE_NEWLINE_AT_EOF" value="true" />
    <option name="REMOVE_TRAILING_BLANK_LINES" value="true" />
    <option name="IS_LEADING_WHITESPACES_SHOWN" value="false" />
    <option name="IS_INNER_WHITESPACES_SHOWN" value="false" />
  </component>
  <component name="TodoConfiguration">
    <pattern pattern="\btodo\b.*" />
    <pattern pattern="\bfixme\b.*" />
  </component>
</application>